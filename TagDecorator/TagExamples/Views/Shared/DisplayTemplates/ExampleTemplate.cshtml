@model TagExamples.Models.ExampleTemplate

<div class="row">
    <div class="col-6">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.HtmlView)
            </dt>

            <dd>
                @Html.Partial("HtmlCode", Model.HtmlView)
            </dd>
        </dl>
    </div>
    <div class="col-6">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.CodeView)
            </dt>

            <dd>
                @Html.Partial("CSharpCode", Model.CodeView)
            </dd>
        </dl>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.ResultView)
            </dt>

            <dd>
                @Html.Partial("~/Views/Buttons/Bs4ButtonsViewPartial.cshtml")
            </dd>
        </dl>
    </div>
</div>